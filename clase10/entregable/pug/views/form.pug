head
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous")
body 
    button(type="button" class="btn btn-outline-dark")
        a(href="/productos")='Ir a ver productos cargados'
    h1='Ingresar nuevo producto'
    form(action="/productos/" method="post")
        input(type="text" name="title" id="title" placeholder='Ingrese el titulo')
        input(type="number" name="price" id="price" placeholder='Ingrese el precio')
        input(type="text" name="img" id="img" placeholder='Ingrese la url de la imagen')
        input(type="submit" value="enviar")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous")

